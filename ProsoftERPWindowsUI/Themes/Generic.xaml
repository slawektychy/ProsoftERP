<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:uic="clr-namespace:Prosoft.WindowsUI.UIControls">

    <Style TargetType="{x:Type uic:UILabel}" BasedOn="{StaticResource {x:Type Label}}">
        <!--<Setter Property="FontSize" Value="16"/>-->
    </Style>

    <Style x:Key="LabelHeader_1" TargetType="{x:Type uic:UILabel}" BasedOn="{StaticResource {x:Type Label}}">
        <Setter Property="FontSize" Value="16"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>



</ResourceDictionary>
